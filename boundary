FoamFile
{
    version     2.0;
    format      ascii;
    class       polyBoundaryMesh;
    location    "constant/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

3 //patchの数
(
    fixedWalls
    {
        type            wall;
        inGroups        1(wall);
        nFaces          40;
        startFace       760;
    }
    inletAndoutlet //patchの名前
    {
        type            patch; //patchのtype
        inGroups        1(patch);
        nFaces          40; //patchを構成する面の数
        startFace       800; //面のリストにおけるpatchが始まる面番号
    }
    frontAndBack
    {
        type            empty;
        inGroups        1(empty);
        nFaces          800;
        startFace       840;
    }
)

